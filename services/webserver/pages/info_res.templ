templ (p InfoRes) View() {
	@layouts.Base{
		Pages:       p.Pages,
		PageCurrent: "resources",
		Title:       "Ethan Thoma / Resources",
	}.View() {
		<article class="row-start-1 col-[2/-2]">
			<header class="font-mono">
				<h1 class="font-bold">
					ETHAN THOMA - INFORMATIONAL RESOURCES
				</h1>
				<br/>
				<hr class="border-t-2 border-black border-dashed"/>
				<br/>
			</header>
			<section>
				<p>
					This page is a collection of resource links about machine 
					learning (ML), programming language theory, and so-called 
					"toy" languages that I find interesting. Some of these 
					resources have been read, some them have not.
				</p>
				<br/>
				<p>
					The ML section consists almost entirely of research papers, 
					so they may not be generally accessible. The programming 
					language (theory) section is mostly comprised of links to 
					project pages or GitHub; thus, more developer friendly.
				</p>
			</section>
			<br/>
			<hr class="border-t-2 border-black border-dashed"/>
			<br/>
			<section>
				<table class="table-fixed [&_td]:align-top table-gap-x-[3ch]">
					<thead><tr><th colspan="2" class="pb-[1em]">Machine Learning (ML)</th></tr></thead>
					<tbody>
						for index, item := range items_ml {
							<tr>
								<td scope="col" class="w-[1%]">{ fmt.Sprintf("%03d", index) }</td>
								<td scope="col" class="w-full">
									<a href={ templ.URL(item.URL) } class="font-semibold hover:underline text-blue hover:text-darkblue">
										{ item.Title }<span class="whitespace-nowrap">&nbsp;<svg class="inline ml-0.5 w-3 h-3 mr-[-2px]" fill="currentColor" viewBox="0 0 16 16"><path fill-rule="evenodd" d="M14 2.5a.5.5 0 0 0-.5-.5h-6a.5.5 0 0 0 0 1h4.793L2.146 13.146a.5.5 0 0 0 .708.708L13 3.707V8.5a.5.5 0 0 0 1 0v-6z"></path></svg></span>
									</a>
								</td>
							</tr>
						}
					</tbody>
					<td colspan="3"><hr class="border-t-2 border-black border-dashed my-[1em]"/></td>
					<thead><tr><th colspan="2" class="pb-[1em]">Programming Language(s) and Theory (PLT)</th></tr></thead>
					<tbody>
						for index, item := range items_plt {
							<tr>
								<td scope="col" class="w-[1%]">{ fmt.Sprintf("%03d", index) }</td>
								<td scope="col" class="w-full">
									<a href={ templ.URL(item.URL) } class="font-semibold hover:underline text-blue hover:text-darkblue">
										{ item.Title }<span class="whitespace-nowrap">&nbsp;<svg class="inline ml-0.5 w-3 h-3 mr-[-2px]" fill="currentColor" viewBox="0 0 16 16"><path fill-rule="evenodd" d="M14 2.5a.5.5 0 0 0-.5-.5h-6a.5.5 0 0 0 0 1h4.793L2.146 13.146a.5.5 0 0 0 .708.708L13 3.707V8.5a.5.5 0 0 0 1 0v-6z"></path></svg></span>
									</a>
								</td>
							</tr>
						}
					</tbody>
				</table>
			</section>
		</article>
	}
}
